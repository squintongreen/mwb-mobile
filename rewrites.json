[
    {
        "from": "js/*",
        "to": "js/*"
    },
    {
        "from": "css/*",
        "to": "css/*"
    },
    {
        "from": "fonts/*",
        "to": "fonts/*"
    },
    {
        "from": "img/*",
        "to": "img/*"
    },
    {
        "from": "images/*",
        "to": "images/*"
    },

    {
        "from": "vendor/*",
        "to": "vendor/*"
    },
    {
        "from": "_session",
        "to": "/_session"
    },
    {
        "from": "_list/*",
        "to": "_list/*"
    },
    {
        "from": "_view/*",
        "to": "_view/*"
    },
    {
        "from": ":id/*",
        "to": "_show/_/:id",
        "query": {
            "key": "*"
        }
    }
]